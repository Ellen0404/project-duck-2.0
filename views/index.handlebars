<div class="uk-section">
    <div class="uk-container">
        <div class="uk-grid-match uk-child-width-1-4@m" uk-grid>
            <div>
                <img src="assets/images/HoodUp_1x1.png" alt="hood up" id="hoodUp">
            </div>
            <div>
                <img src="assets/images/MercHeadlight_1x1.png" alt="headlights" id="headlights">
            </div>
            <div>
                <img src="assets/images/Keys_1x1.png" alt="Keys" id="Keys">
